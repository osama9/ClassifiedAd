


<table class="table" ng-cloak>
	<thead>
    <tr>
      <th></th>
      <th></th>
      <th>{{strings[0]}}</th>
      <th><%= I18n.t :date %></th>
    </tr>
  </thead>

   <tbody>
         <tr ng-repeat="classified in vm.classifiedList">
        
          <td class="post-image"><img src="{{classified.images[0].path.url}}" width="150" height="100" /></td>
          
          <td>
          	<h4><a href="/{{vm.locale}}/classified_ads/{{classified.id}}"> {{classified.title}}</a></h4>
          	<p>{{classified.description}}</p>
          </td>
          <td class="post-city">
          	<h5>
          	{{classified.city.english_name}}
          	</h5>
          </td>
          <td>{{classified.created_at | date:"dd/MM/yyyy 'at' h:mma"}}</td>
        
      </tr>
 
  </tbody>
</table>


<script type="text/javascript">
  window.I18n = <%= I18n.backend.send(:translations).to_json.html_safe %>
  console.log(I18n["en"]["hello"]);
</script>

